---
interface Props {
    nombre?: string
    profesion?: string
}
const {nombre="default", profesion="default"} = Astro.props;
---

<div class="card">
    <h2>{nombre}</h2>
    <p>{profesion}</p>
</div>

<style>
    .card{
        width: 200px;
        border: 2px solid black;
        text-align: center;
        margin-bottom: 10px;
        border-radius: 5px;
        background-color: skyblue;
    }
</style>